{
    "compilerOptions": {
        "lib": ["ESNext", "DOM"],
        "target": "ESNext",
        "module": "ESNext",
        "moduleDetection": "force",
        "moduleResolution": "bundler",
        "verbatimModuleSyntax": true,
        "resolveJsonModule": true,
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true,
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}

